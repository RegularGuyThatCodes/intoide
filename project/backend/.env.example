# Database
DATABASE_URL="postgresql://postgres:password@localhost:5432/intoide"

# JWT
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"

# Server
PORT=3001
NODE_ENV=development

# Stripe
STRIPE_SECRET_KEY="sk_test_your_stripe_secret_key"
